{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/models(.*)",
      "dest": "/api/models/Favorite.js"
    },
    {
      "src": "/api/models(.*)",
      "dest": "/api/models/Search.js"
    },
    {
      "src": "/api/models(.*)",
      "dest": "/api/models/User.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ]
}
